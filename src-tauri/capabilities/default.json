{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "http:default",
      "description": "Capability for HTTP requests",
      "allow": [
        {
          "url": "https://api.videosdk.live/*"
        }
      ]
    },
    "dialog:default",
    "notification:default"
  ]
}